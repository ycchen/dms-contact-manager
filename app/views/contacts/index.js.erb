<% if (params[:view].nil? || params[:view] == "grid")%>
$('#contact_list').append('<%= j render :partial => "datalist" %>');
<% else %>
$('#contact_list').append('<%= j render :partial => "tabledata" %>');
<% end %>
$('.pagination').replaceWith('<%= j paginate(@contacts) %>');
if ($('.pagination .next').length ==0)
	$('.pagination').hide()
