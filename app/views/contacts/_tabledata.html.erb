<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.last_name %></td>
    <td><%= contact.first_name %></td>
    <td><%= contact.company %></td>
    <td><%= contact.business_phone %></td>
    <td><%= contact.email %></td>
    <td><%= link_to 'Show', contact, class: "btn btn-primary" %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact),class: "btn btn-primary"  %></td>
    <td><%= link_to 'Destroy', contact,class: "btn btn-primary" , method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
